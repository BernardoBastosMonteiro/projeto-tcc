<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>controler</servlet-name>
        <servlet-class>controler.controler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlerLogin</servlet-name>
        <servlet-class>controler.ControlerLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagamentoController</servlet-name>
        <servlet-class>controler.PagamentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>controlerAdm</servlet-name>
        <servlet-class>controler.controlerAdm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagamentoControler</servlet-name>
        <servlet-class>controler.PagamentoControler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PedidoController</servlet-name>
        <servlet-class>controler.PedidoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controler.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdicionarController</servlet-name>
        <servlet-class>controler.AdicionarController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>concluidoController</servlet-name>
        <servlet-class>controler.concluidoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>avaliacoesController</servlet-name>
        <servlet-class>controler.termosLegaisController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>termosController</servlet-name>
        <servlet-class>controler.termosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarrinhoController</servlet-name>
        <servlet-class>controler.CarrinhoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcompanharController</servlet-name>
        <servlet-class>controler.AcompanharController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GerenciarController</servlet-name>
        <servlet-class>controler.GerenciarController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GerencimentoController</servlet-name>
        <servlet-class>controler.GerencimentoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gerenciarUsuariosController</servlet-name>
        <servlet-class>controler.gerenciarUsuariosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>controler.AdminController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controler</servlet-name>
        <url-pattern>/viagens</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerLogin</servlet-name>
        <url-pattern>/ControlerLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerLogin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlerLogin</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PagamentoController</servlet-name>
        <url-pattern>/PagamentoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>controlerAdm</servlet-name>
        <url-pattern>/controlerAdm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PagamentoControler</servlet-name>
        <url-pattern>/PagamentoControler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PedidoController</servlet-name>
        <url-pattern>/PedidoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/HomeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/teste</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdicionarController</servlet-name>
        <url-pattern>/AdicionarController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>concluidoController</servlet-name>
        <url-pattern>/concluidoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>termosController</servlet-name>
        <url-pattern>/termosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarrinhoController</servlet-name>
        <url-pattern>/CarrinhoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AcompanharController</servlet-name>
        <url-pattern>/AcompanharController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GerenciarController</servlet-name>
        <url-pattern>/GerenciarController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GerencimentoController</servlet-name>
        <url-pattern>/GerencimentoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gerenciarUsuariosController</servlet-name>
        <url-pattern>/gerenciarUsuariosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
